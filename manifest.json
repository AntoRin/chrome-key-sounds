{
  "manifest_version": 3,
  "name": "Mechanical Keyboard Emulator",
  "version": "0.1.0",
  "web_accessible_resources": [
    {
      "resources": ["KeySounds/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "/popup/popup.html"
  }
}
